{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3000",
    "build": "node scripts/vercel-build.js",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:setup": "prisma db push --skip-generate && prisma generate",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit-fix": "npm audit fix",
    "security:audit-report": "npm audit --json > audit-report.json && npm audit > audit-summary.txt",
    "security:licenses": "npx license-checker --summary",
    "security:licenses-report": "npx license-checker --json > license-report.json && npx license-checker --summary > license-summary.txt",
    "security:secrets": "echo Running local secret scan... && powershell -Command \"Get-ChildItem -Recurse -Include *.ts,*.js,*.tsx,*.jsx | Select-String -Pattern '(api[_-]?key|secret[_-]?key|access[_-]?token|auth[_-]?token)' -CaseSensitive:$false\" || echo No obvious secrets found",
    "security:check-all": "npm run security:audit && npm run security:licenses && npm run security:secrets",
    "security:pre-commit": "npm run lint && npm run security:check-all && npx tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.5",
    "@clerk/themes": "^2.4.11",
    "@prisma/client": "^6.13.0",
    "@react-google-maps/api": "^2.19.3",
    "@react-pdf/renderer": "^4.3.0",
    "@react-three/drei": "^10.7.5",
    "@react-three/fiber": "^9.3.0",
    "@slack/webhook": "^7.0.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^11.18.2",
    "isomorphic-dompurify": "^2.26.0",
    "jose": "^6.0.13",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "next": "15.3.2",

    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-markdown": "^10.1.0",
    "react-query": "^3.39.3",
    "react-slick": "^0.29.0",
    "recharts": "^3.0.2",
    "resend": "^6.0.3",
    "slick-carousel": "^1.8.1",
    "svix": "^1.73.0",
    "tailwind-merge": "^2.0.0",
    "three": "^0.180.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jquery": "^3.5.33",
    "@types/node": "^20",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@types/react-slick": "^0.23.10",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "postcss": "^8.5.5",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
